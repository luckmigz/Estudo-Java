import java.Math;
import javax.swing.*;

public class Ex3{ 
    public static void main(String[] args){
       String str = JOptionPane.showInputDialog("Qual o valor?");
       int c = Integer.parseInt(str);
       if( c < 0 ){
            posi
       } 
    }
    public static String posi(int a){
        int b = Math.sqtr(a);
        String s = " A raiz é :" + b;
        return s;
    }   
    public static String nega(int a){ 
        int b = a*a; 
        String s = " O quadrado é :" + b;
        return s;
    }
}
